<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="">
	<meta name="author" content="">
	<title>Home | E-Shopper</title>
	<link href="~/css/bt3/bootstrap.min.css" rel="stylesheet">
	@* <link href="~/css/font-awesome.min.css" rel="stylesheet"> *@

	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.0.1/css/font-awesome.css" rel="stylesheet">
	<!-- Thông báo Sweet Alert-->
	<link href="https://cdn.jsdelivr.net/npm/sweetalert2@11.14.0/dist/sweetalert2.min.css" rel="stylesheet">

	<link href="~/css/prettyPhoto.css" rel="stylesheet">
	<link href="~/css/price-range.css" rel="stylesheet">
	<link href="~/css/animate.css" rel="stylesheet">
	<link href="~/css/main.css" rel="stylesheet">
	<link href="~/css/responsive.css" rel="stylesheet">
	<!--[if lt IE 9]>
	<script src="~/js/html5shiv.js"></script>
	<script src="~/js/respond.min.js"></script>
	<![endif]-->
	<link rel="shortcut icon" href="~/images/ico/favicon.ico">
	<link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/images/ico/apple-touch-icon-144-precomposed.png">
	<link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/images/ico/apple-touch-icon-114-precomposed.png">
	<link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/images/ico/apple-touch-icon-72-precomposed.png">
	<link rel="apple-touch-icon-precomposed" href="~/images/ico/apple-touch-icon-57-precomposed.png">


</head><!--/head-->

<body>
	
	<partial name="_HeaderPartial" />

	<section>
		<div class="container">
			<div class="row">
				@Html.Partial("_NotificationPartial")
				@RenderBody()
			</div>
		</div>
	</section>

	<partial name="_FooterPartial" />


	<div id="fb-root"></div>
	<script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v20.0" nonce="yOpFfkjR"></script>

	@* <script src="~/js/jquery.js"></script> *@

	<!-- Start Script để điều khiển việc ẩn/hiện với hiệu ứng nút "Xem thêm" -->
	<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
	<script src="~/js/site-brand.js"></script>
	<script src="~/js/site-category.js"></script>

	<!-- End Script để điều khiển việc ẩn/hiện với hiệu ứng nút "Xem thêm" -->


	<script src="~/js/bt3/bootstrap.min.js"></script>
	<script src="~/js/jquery.scrollUp.min.js"></script>
	<script src="~/js/price-range.js"></script>
	<script src="~/js/jquery.prettyPhoto.js"></script>
	<script src="~/js/main.js"></script>

	<!-- Thông báo Sweet Alert-->
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.14.0/dist/sweetalert2.all.min.js"></script>
	<script>
		$(function () {
			var message = '@TempData["success"]';
			if (message != '')
				Swal.fire(message);
		});
	</script>




	@await RenderSectionAsync("Scripts", required: false)

</body>
</html>